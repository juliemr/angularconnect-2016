{
  "name": "angularconnect-2016-test-project",
  "version": "0.0.0",
  "description": "Examples for AngularConnect 2016",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juliemr/angularconnect-2016"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "copy": "cp src/{index.html,styles.css,system-config.js} dist/",
    "copytemplates": "cp src/app/{*.html,*.css} dist/app/",
    "build": "tsc -p src && npm run copy && npm run copytemplates",
    "watch": "tsc --watch",
    "serve": "http-server -p 9095 -c-1",
    "test": "karma start config/karma.conf.js"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/router": "3.0.1",
    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.2.34",
    "clang-format": "^1.0.35",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "http-server": "^0.8.5",
    "jasmine": "2.5.2",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "rimraf": "^2.4.3",
    "typescript": "2.0.3"
  }
}
